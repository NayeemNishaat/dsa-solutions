{
  "name": "esnext-template",
  "version": "1.0.0",
  "description": "A template for using ESNEXT features with typescript.",
  "type": "module",
  "scripts": {
    "dev": "nodemon -q -x node --no-warnings --experimental-strip-types src/index.ts",
    "start": "node dist/index.js",
    "test": "nodemon -q -x NODE_ENV=development node --no-warnings --experimental-strip-types src/problem.test.ts"
  },
  "devDependencies": {
    "@types/node": "13.9.0",
    "ts-node": "^10.9.1"
  },
  "keywords": [
    "NodeJs",
    "TypeScript",
    "TSNode"
  ],
  "author": "Nayeem",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
